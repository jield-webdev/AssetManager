{
  "name": "jield-webdev/laminas-assetmanager",
  "description": "An assetmanager module for Laminas",
  "license": "BSD-2-Clause",
  "type": "library",
  "keywords": [
    "asset",
    "asset manager",
    "laminas"
  ],
  "homepage": "https://github.com/jield-webdev/laminas-assetmanager",
  "authors": [
    {
      "name": "Wesley Overdijk",
      "email": "r.w.overdijk@gmail.com",
      "homepage": "http://blog.spoonx.nl/"
    },
    {
      "name": "Marco Pivetta",
      "email": "ocramius@gmail.com",
      "homepage": "http://ocramius.github.com/"
    },
    {
      "name": "Johan van der Heide",
      "email": "info@jield.nl",
      "homepage": "https://jield.nl"
    }
  ],
  "require": {
    "php": "^7.2",
    "scssphp/scssphp": "~1.0",
    "laminas/laminas-modulemanager": "^2.7.2",
    "laminas/laminas-stdlib": "^2.7.7 || ^3.0.1",
    "laminas/laminas-servicemanager": "^2.7.6 || ^3.1.1",
    "laminas/laminas-loader": "^2.5.1",
    "laminas/laminas-eventmanager": "^2.6.3 || ^3.0.1",
    "laminas/laminas-mvc": "^2.7.9 || ^3.0.2",
    "assetic/framework": "~2.0.0"
  },
  "require-dev": {
    "laminas/laminas-http": "^2.5.4",
    "laminas/laminas-view": "^2.8.1",
    "laminas/laminas-cache": "^2.7.1",
    "laminas/laminas-console": "^2.6",
    "phpunit/phpunit": "^9",
    "squizlabs/php_codesniffer": "~1.5.0"
  },
  "suggest": {
    "laminas/laminas-mvc-console": "laminas-mvc-console provides the ability to expose laminas-mvc as a console application"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev",
      "dev-develop": "1.1-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "AssetManager\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AssetManagerTest\\": "tests"
    }
  }
}
